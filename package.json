{
  "name": "yellfy-cli",
  "version": "1.0.0",
  "description": "Command line utility to install Yellfy",
  "license": "MIT",
  "repository": "mrmlnc/yellfy-cli",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "bin": {
    "yellfy": "./cli.js"
  },
  "scripts": {
    "test": "rimraf tmp && xo && mocha test.js --timeout=15000"
  },
  "files": [
    "lib",
    "templates",
    "cli.js"
  ],
  "keywords": [
    "yellfy",
    "cli"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "elegant-status": "^1.0.0",
    "fs-extra": "^3.0.0",
    "meow": "^3.7.0",
    "promise": "^7.1.1",
    "read-pkg-up": "^2.0.0"
  },
  "devDependencies": {
    "mocha": "^3.3.0",
    "rimraf": "^2.5.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ]
    },
    "envs": [
      "node",
      "mocha"
    ]
  }
}
